<div class="garden-container">
    <div class="garden-header col-md-10" >
        <h2>My Garden</h2>
    </div>
    <div class='form-group'>
    <label for="plant-input">search plants</label>
    <input class="form-input" type="search" id="plant-input" />
    <button class="btn btn-primary" type="submit">add plant</button>
</div>    
    {{#each plants as |plant|}}
    <div class="plant col-md-5">
        <p>
            {{plant.id}}
            {{plant.plant_name}}
        </p>
    </div>
    {{/each}}
</div>
